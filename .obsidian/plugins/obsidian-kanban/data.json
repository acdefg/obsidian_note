{
  "show-relative-date": true,
  "hide-date-display": false,
  "hide-date-in-title": true,
  "date-picker-week-start": 1,
  "date-format": "YYYY-MM-DD",
  "date-display-format": "MM-DD",
  "prepend-archive-date": true,
  "date-colors": [
    {
